{
    "name": "magewirephp/portman",
    "description": "Portman is a command-line utility that simplifies porting PHP libraries between frameworks.",
    "keywords": [
        "Porting",
        "PHP"
    ],
    "type": "project",
    "version": "0.5.3",
    "license": "MIT",
    "authors": [
        {
            "name": "Justin van Elst",
            "email": "justin@publicus.nl"
        }
    ],
    "require-dev": {
        "spatie/temporary-directory": "^2.3",
        "composer/composer": "^2.8",
        "friendsofphp/php-cs-fixer": "^3.65",
        "illuminate/log": "^11.34",
        "illuminate/validation": "^11.34",
        "larastan/larastan": "^2.9",
        "laravel-zero/framework": "^11.0",
        "laravel-zero/phar-updater": "^1.4",
        "laravel/pint": "^1.18",
        "mockery/mockery": "^1.6",
        "nikic/php-parser": "^5.3",
        "pestphp/pest": "^3.5",
        "rector/rector": "^1.2",
        "spatie/file-system-watcher": "^1.2",
        "spatie/ray": "^1.41",
        "webmozart/glob": "^4.7",
        "spatie/laravel-data": "^4.13"
    },
    "autoload": {
        "files": [
            "app/helpers.php"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "bin": [
        "builds/portman"
    ]
}
