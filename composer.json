{
    "name": "magewirephp/poortman",
    "description": "Poortman your porting aid for PHP",
    "keywords": ["Porting", "PHP"],
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Justin van Elst",
            "email": "justin@publicus.nl"
        }
    ],
    "require": {
        "php": "^8.2.0",
        "friendsofphp/php-cs-fixer": "^3.59",
        "laravel-zero/framework": "^11.0.0",
        "nikic/php-parser": "^5.0",
        "rector/rector": "^1.1",
        "spatie/file-system-watcher": "^1.2"
    },
    "require-dev": {
        "laravel/pint": "^1.15.2",
        "mockery/mockery": "^1.6.11",
        "pestphp/pest": "^2.34.7",
        "spatie/ray": "^1.41"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "bin": ["poortman"]
}
